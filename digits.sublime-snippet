	<content><![CDATA[
vector<int> digits(int n){
	vector<int> ans;
	while(n){
		ans.push_back(n%10);
		n/=10;
	}
	reverse(ans.begin(), ans.end());
	return ans;
}
]]></content>
	<!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
	<tabTrigger>digits</tabTrigger>
